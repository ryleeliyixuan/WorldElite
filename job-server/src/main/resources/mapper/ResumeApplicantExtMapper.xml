<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.worldelite.job.mapper.ResumeApplicantMapper">
  <select id="selectByUserId" parameterType="java.lang.Long" resultMap="BaseResultMap">
    select 
    <include refid="Base_Column_List" />
    from t_resume_applicant
    where user_id = #{userId,jdbcType=INTEGER}
  </select>
  <select id="selectByResumeId" parameterType="java.lang.Long" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List" />
    from t_resume_applicant
    where resume_id = #{resumeId,jdbcType=INTEGER}
  </select>
  <select id="selectAndList" parameterType="com.worldelite.job.entity.ResumeApplicant" resultMap="BaseResultMap">
    select
    <include refid="Base_Column_List" />
    from t_resume_applicant
    <where>
      <if test="userId != null">
        and user_id = #{userId,jdbcType=BIGINT}
      </if>
      <if test="resumeId != null">
        and resume_id = #{resumeId,jdbcType=BIGINT}
      </if>
    </where>
  </select>
</mapper>